{
   "compilerOptions": {
      "lib": [
         "es6"
      ],
      "types": ["node"],
      "target": "es6",
      "module": "commonjs",
      "moduleResolution": "node",
      "outDir": "./build",
      "allowSyntheticDefaultImports": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "sourceMap": true,
      "esModuleInterop": true
   },
   "exclude": [
      "src/imports/database/migration",
      "src/imports/database/seeds"
   ]
}
